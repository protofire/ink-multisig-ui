{
    "builds": [
      { "src": "package.json", "use": "@vercel/next", "config": { "distDir": "/" } },
      { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "storybook-static" } }
    ],
    "routes": [
        { "src": "/storybook(/.*)?", "dest": "/storybook-static$1" },
        { "src": "/(?!storybook)(.*)", "dest": "/$1" }
      ]
  }